:@template"±#.view-apartments {
    .view-filters { @include shadow; background-color: #ffffff; margin: 0px 0px 30px 0px; height:42px; padding: 10px;
      form { float: right; margin: 0px;
        .views-exposed-widgets { margin: 0px;
          .views-exposed-widget { padding-top: 0px;
            label { color: $black; display: inline-block; padding-right: 15px; padding-top: 10px; vertical-align: top; }
            button { background-image: none; color: #ffffff; font-size: 16px; height: 42px; margin: 0px !important; padding: 7px 15px; }
            select { width: 100px !important; }
            .controls { display: inline; }
            .chzn-container { border: 1px solid $gray-light; 
              .chzn-drop { border: 1px solid $gray-light; border-top: 0px; margin-left: -1px; width: 100px !important; }
            }

            &.views-submit-button { padding-right: 0px; }
          }
        }
      }
    }

	.item { @include shadow; background-color: #fff; margin-bottom: 30px; padding-bottom: 10px; position: relative;
    .views-field-field-image { height: 169px; overflow: hidden;
      .field-content {
        @include transition(opacity .2s linear, width .2s linear);

        width: 100%;
        &:hover {
          opacity: .8; width: 104%;
          img { margin-left: -2%; margin-top: -2%; }
        }
      }
      img { @include transition(margin-left .2s linear, margin-top .2s linear); height: auto; width: 100%; }
    }
    h2 { line-height: 22px; margin: 10px 0px 0px 0px; padding: 0px 15px;
        a { @include transition(color .1s ease-in); color: $blue; font-size: 18px; font-weight: normal;
            &:hover { color: darken($blue, 10%); text-decoration: none; }
        }
    }
    .content { margin-right: 30px; }
		.views-field-field-location { padding: 0px 15px; }
		.views-field-field-price { background-color: #ffffff; color: $black; font-size: 18px; padding: 5px 16px; position: absolute; right: 15px; bottom: 105px; }
		.views-field-field-area { color: $black; float: left; margin-top: 8px; 
			.views-label { display: inline; font-weight: bold; padding: 0px 0px 0px 15px; }
			.field-content { display: inline; }
		}
		.views-field-field-bedrooms { float: right; margin-right: 10px; padding-top: 8px; 
			.field-content { background-image: url('../img/icons/bedrooms.png'); background-position: left center; background-repeat: no-repeat; background-size: 21px 12px; color: $black; padding-left: 30px;
        @media (-webkit-min-device-pixel-ratio: 2), (-moz-min-device-pixel-ratio: 2) {
          background-image: url('../img/icons/bedrooms@2x.png');
        }
      }
		}
		.views-field-field-bathrooms { float: right; margin-right: 15px; padding-top: 8px;
			.field-content { background-image: url('../img/icons/bathrooms.png'); background-position: left center; background-repeat: no-repeat; background-size: 14px 20px; color: $black; padding-left: 25px;
        @media (-webkit-min-device-pixel-ratio: 2), (-moz-min-device-pixel-ratio: 2) {
          background-image: url('../img/icons/bathrooms@2x.png');
        }
      }
		}
	}
  
  .row {
    .span12, .span9 {
      .row {
        &:last-child {
          .span3 { margin-bottom: 0px; }
        }
      }
    }
  }
}

@media (min-width: 1200px) {
  .view-apartments {
    .views-field-field-image { height: 205px; }
  }
}

@media (min-width: 768px) and (max-width: 979px) {
	.view-apartments {
		.span3 {
			h2 { 
				a { font-size: 14px; }
			}
      .views-field-field-bathrooms { float: left; margin-left: 10px; }
      .views-field-field-bedrooms { float: left; margin-left: 10px; }
			.views-field-field-area { display: none; }
		}
	}
}

@media (max-width: 767px) {
  .view-apartments { padding-left: 20px; padding-right: 20px;
    .view-filters {
      form {
        .views-exposed-widgets {
          .views-exposed-widget {
            label { display: none; }
          }
        }
      }
    }

    .span3 { margin-bottom: 30px; overflow: hidden; }

    .row {
      .span9 {
        .row {
          &:last-child {
            .span3 { margin-bottom: 30px; }
          }
        }
      }
    }
  }
}

#block-views-apartments-block-2,
#block-views-apartments-block-1 {
  .content {
    .views-row { @include clearfix; @include shadow; background-color: #ffffff; margin-bottom: 15px;
      .views-field-field-image { float: left; height: auto;
        img { margin-right: 10px; width: 80px; }
      }
      .views-field-title {
        h3 { line-height: 18px; margin: 6px 0px 2px 0px;
          a { font-size: 12px; text-decoration: none; }
        }
      }
      .views-field-field-price { display: none; color: $blue-dark; font-weight: bold; }
      &:last-child { margin-bottom: 0px; }
    }
  }
}

@media (min-width: 1200px) {
  #block-views-apartments-block-1 {
    .content {
      .views-row { font-size: 12px;
        .views-field-field-image {
          img { width: 100px; }
        }
        .views-field-title { margin-top: 10px;
          h3 { margin-top: -5px;
            a { font-size: 14px; }
          }
        }
        .views-field-field-price { display: block; }
      }
    }
  }
}

@media (min-width: 768px) and (max-width: 979px) {
  #block-views-apartments-block-1 {
    .content {
      .views-row { padding-bottom: 5px;
        .views-field-field-image { float: none;
          img { width: 100%; }
        }
        .views-field-title { padding: 0px 10px;
          a { font-size: 12px; }
        }
        .views-field-field-location { font-size: 14px; padding: 0px 10px; }
        .views-field-field-price { display: block; font-size: 14px; padding: 0px 10px; }
      }
    }
  }
}

@media (max-width: 767px) {
  #block-views-apartments-block-1 {
    .content {
      .view { padding: 0px; }
    }
  }
}

.listing-row,
.view-display-id-page_2 {
  .row {
    .span9 {
      > .row { margin-bottom: 30px;         
        &:last-child { margin-bottom: 0px; }
        .item { @include clearfix; @include shadow; background-color: #fff; margin-bottom: 0px; padding: 0px; position: relative;
          .span6 { padding-bottom: 10px; }
          .title-price { padding-bottom: 3px; padding-top: 17px;
            .title {
              a { color: $blue; font-size: 24px; font-weight: normal;
                &:hover { text-decoration: none; }
              }
            }
            .price { clear: right; color: $black; float: right; font-size: 24px; text-align: center; }
          }
          .location { background-image: url('../img/border.png'); background-position: left bottom; background-repeat: repeat-x; color: $gray; font-size: 18px; line-height: 1; margin-bottom: 12px; margin-right: 30px; padding-bottom: 12px;  }
          .content { color: $gray; }
          .area {
            .label { @include border-radius(0px); @include text-shadow(none); background-color: transparent; color: $black; font-size: 14px; padding: 0px; }
            display: inline-block; margin-right: 10px;
          }
          .bedrooms { background-image: url('../img/icons/bedrooms.png'); background-position: left center; background-repeat: no-repeat; background-size: 21px 12px; color: $black; display: inline-block; margin-right: 10px; padding-left: 30px;
            @media (-webkit-min-device-pixel-ratio: 2), (-moz-min-device-pixel-ratio: 2) {
              background-image: url('../img/icons/bedrooms@2x.png');
            }
          }
          .bathrooms { background-image: url('../img/icons/bathrooms.png'); background-position: left center; background-repeat: no-repeat; background-size: 14px 20px; color: $black; display: inline-block; margin-right: 10px; padding-left: 25px;
            @media (-webkit-min-device-pixel-ratio: 2), (-moz-min-device-pixel-ratio: 2) {
              background-image: url('../img/icons/bathrooms@2x.png');
            }
          }

          .views-field-field-bedrooms { margin: 0px; padding: 0px;
            .field-content { padding-left: 0px; }
          }        
        }        
      }
      // &:last-child { margin-bottom: 30px; }
    }
  }
}

@media (min-width: 768px) and (max-width: 979px) {
  .view-display-id-page_2 {
    .row {
      .span9 {
        .row {
          .item {
            .title-price { padding-top: 9px;
              .title {
                a { font-size: 16px; }
              }
              .price { font-size: 16px; }
            }
            .content {
              p { line-height: 20px; }
            }
            .location { font-size: 14px; }
          }
        }
      }
    }
  }
}

@media (max-width: 767px) {
  .view-display-id-page_2 {
    .row {
      .span9 {
        .row {
          .item {
            .title-price {
              .title { float: left; width: 50%;
                a { font-size: 18px; }
              }
              .price { float: right; font-size: 18px; text-align: right; width: 50%; }
            }
            .location { margin-right: 0px; }
            .views-field-field-bedrooms {
              .field-content { background-image: none; }
            }
            .span3 { margin: 0px; }
            .span6 { padding: 10px; }
          }
        }
      }
    }
  }
